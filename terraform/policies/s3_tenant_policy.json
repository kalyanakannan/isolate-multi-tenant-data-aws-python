{
    "Effect": "Allow",
    "Action": ["s3:ListBucket"],
    "Resource": ["arn:aws:s3:::{{bucket}}"],
    "Condition": {
      "StringLike": {
        "s3:prefix": ["{{tenant}}", "{{tenant}}/", "{{tenant}}/*"]
      }
    }
},
{
  "Effect": "Allow",
  "Action": ["s3:GetObject", "s3:PutObject", "s3:DeleteObject"],
  "Resource": ["arn:aws:s3:::{{bucket}}/{{tenant}}/*"]
}
